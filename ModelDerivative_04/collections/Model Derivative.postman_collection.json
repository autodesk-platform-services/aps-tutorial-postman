				{
					"name": "Task 3 - Translate to SVF2",
					"item": [
						{
							"name": "Start a Translation Job",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"var body = JSON.parse(responseBody);",
											"pm.environment.set(\"t4_url_safe_urn_of_source\", body[\"urn\"]);"
										],
